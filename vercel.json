{
  "version": 2,
  "builds": [
    {
      "src": "api/server.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": "**/*" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/server.ts"
    }
  ]
}
